{
  "name": "extract-us-city",
  "version": "0.0.1",
  "description": "A library that extracts City, State and Zip information from text and provides a structured response.",
  "main": "dist/index.min.js",
  "scripts": {
    "test": "eslint index.js && node test.js",
    "clean":"rm -r dist && mkdir dist",
    "compress": "terser index.js -o dist/index.min.js -c -m --source-map",
    "copy-db": "cp ./city-db.json ./dist/city-db.json",
    "build": "npm run test && npm run clean && npm run compress && npm run copy-db"
  },
  "keywords": [
    "parse",
    "extract",
    "us",
    "city",
    "state",
    "zip",
    "location",
    "nlp",
    "natural language processing"
  ],
  "author": "Joe Motacek",
  "license": "ISC",
  "devDependencies": {
    "terser": "^4.2.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "dependencies": {}
}
